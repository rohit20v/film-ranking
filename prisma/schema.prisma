generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user_movie {
  id         Int      @id @default(autoincrement())
  tconst     String
  name       String
  rating     Decimal
  created_at DateTime
}

model user {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
